#!/usr/bin/env bash

if [ "$(uname -s)" = "Linux" ]; then
    alias open="xdg-open"
fi

export WD=$PWD

for x in 01_hello_world 02_disasm; do
    for y in bin out; do
        if [ ! -d "$WD/$x/$y" ]; then
            mkdir "$WD/$x/$y"
        fi
    done
done

now () {
    date +%s.%N
}

export -f now
